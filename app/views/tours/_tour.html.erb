<div id="<%= dom_id tour %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= tour.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Start date:</strong>
    <%= tour.start_date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">End date:</strong>
    <%= tour.end_date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Track:</strong>
    <%= tour.track %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Note:</strong>
    <%= tour.note %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image1:</strong>
    <%= tour.image1 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image2:</strong>
    <%= tour.image2 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image3:</strong>
    <%= tour.image3 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image4:</strong>
    <%= tour.image4 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Image5:</strong>
    <%= tour.image5 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= tour.user_id %>
  </p>

</div>
