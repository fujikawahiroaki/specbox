  <div class="my-5">
    <%= form.label :maximum_depth %>
    <%= form.text_field :maximum_depth, class: "block shadow rounded-md border border-gray-400 outline-none px-3 py-2 mt-2 w-full" %>
  </div>

  <div class="my-5">
